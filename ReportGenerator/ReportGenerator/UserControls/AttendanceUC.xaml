<UserControl x:Class="ReportGenerator.UserControls.AttendanceUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ReportGenerator"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.DataContext>
        <local:SeminarViewModel/>
    </UserControl.DataContext>
    <Grid>
        <TabControl ItemsSource="{Binding seminars}" x:Name="tabAttendance" HorizontalAlignment="Stretch" Width="Auto" Height="Auto" VerticalAlignment="Stretch">
            <TabItem Content="{Binding SeminarName}"/>
            <TabItem Content="lul"/>

        </TabControl>

    </Grid>
</UserControl>
